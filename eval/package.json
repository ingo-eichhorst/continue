{
  "name": "@continuedev/eval",
  "version": "0.1.0",
  "description": "Benchmarking framework for Continue.dev LLM evaluation",
  "main": "dist/eval/cli/main.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "cd .. && npx tsx eval/cli/main.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "benchmark": "node dist/eval/cli/main.js"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "dockerode": "^4.0.2",
    "ora": "^7.0.1",
    "uuid": "^9.0.1",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/dockerode": "^3.3.41",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engine-strict": true,
  "engines": {
    "node": ">=20.19.0"
  },
  "keywords": [
    "continue",
    "llm",
    "benchmark",
    "evaluation",
    "code-generation"
  ],
  "author": "Continue.dev Team",
  "license": "Apache-2.0"
}
